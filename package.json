{
  "name": "i-demoed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint . --fix",
      "npx prettier --write"
    ]
  },
  "dependencies": {
    "@types/gtag.js": "0.0.4",
    "autoprefixer": "^10.1.0",
    "canvas": "^2.6.1",
    "date-fns": "^2.11.1",
    "gray-matter": "^4.0.2",
    "isomorphic-unfetch": "^3.1.0",
    "jsdom": "^16.4.0",
    "lint-staged": "^10.5.3",
    "next": "^10.0.0",
    "node-fetch": "^2.6.1",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "remark": "^12.0.0",
    "remark-html": "^12.0.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.5",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.6",
    "typescript": "^3.8.3"
  }
}
