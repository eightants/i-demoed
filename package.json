{
  "name": "i-demoed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && npm run build",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint . --fix",
      "npx prettier --write"
    ]
  },
  "dependencies": {
    "@types/gtag.js": "0.0.4",
    "autoprefixer": "^10.1.0",
    "canvas": "^2.11.2",
    "date-fns": "^2.11.1",
    "eslint-config-next": "^13.4.19",
    "gray-matter": "^4.0.2",
    "isomorphic-unfetch": "^3.1.0",
    "jsdom": "^16.4.0",
    "lint-staged": "^10.5.3",
    "next": "^13.4.19",
    "node-fetch": "^2.6.1",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark": "^12.0.0",
    "remark-html": "^12.0.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.5",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "typescript": "^5.2.2"
  }
}
